						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "no line info|no line info",
						"text" : "",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "125|line 125",
						"text" : "desiderava cominciare il cammino, soffrivo",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "no line info|no line info",
						"text" : "",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "126|line 126",
						"text" : "ore di angoscia. Ora è viva in me una speranza di salvezza,",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "no line info|no line info",
						"text" : "",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "127|line 127",
						"text" : "quel legno trionfante cercare",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "no line info|no line info",
						"text" : "",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "128|line 128",
						"text" : "da solo e più spesso di tutti gli altri uomini,",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "no line info|no line info",
						"text" : "",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "129|line 129",
						"text" : "venerarlo degnamente. Questo desiderio",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "no line info|no line info",
						"text" : "",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "130|line 130",
						"text" : "è saldo nel mio cuore, e le mie speranze di soccorso",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "no line info|no line info",
						"text" : "",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "131|line 131",
						"text" : "sono riposte nella Croce. Non ho amici potenti a iosa",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "no line info|no line info",
						"text" : "",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "132|line 132",
						"text" : "nel mondo, no, tutti sono partiti,",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "no line info|no line info",
						"text" : "",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "133|line 133",
						"text" : "hanno lasciato i piaceri terreni per cercare il Re della gloria.",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "no line info|no line info",
						"text" : "",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "134|line 134",
						"text" : "Ora vivono nel regno dei cieli accanto all’alto Padre,",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "no line info|no line info",
						"text" : "",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "135|line 135",
						"text" : "dimorano nella gloria, e io attendo ogni giorno",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "no line info|no line info",
						"text" : "",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "136|line 136",
						"text" : "l’attimo in cui la Croce del Signore,",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "no line info|no line info",
						"text" : "",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "137|line 137",
						"text" : "che prima ho contemplato qui, sulla terra,",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "no line info|no line info",
						"text" : "",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "138|line 138",
						"text" : "mi rapisca da questa vita effimera",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "no line info|no line info",
						"text" : "",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "139|line 139",
						"text" : "e mi porti là dove la gioia è immensa,",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "no line info|no line info",
						"text" : "",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "140|line 140",
						"text" : "la beatitudine nei cieli, là dove il popolo di Dio",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "no line info|no line info",
						"text" : "",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "141|line 141",
						"text" : "siede a banchetto, là dove la felicità è eterna;",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "no line info|no line info",
						"text" : "",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "142|line 142",
						"text" : "e mi ponga dove poi io possa per sempre",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "no line info|no line info",
						"text" : "",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "143|line 143",
						"text" : "dimorare nello splendore, con tutti i santi",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "no line info|no line info",
						"text" : "",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "144|line 144",
						"text" : "bearmi di quelle gioie. Mi sia amico il Signore,",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "no line info|no line info",
						"text" : "",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "145|line 145",
						"text" : "che un tempo qui, sulla terra, ha sofferto",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "no line info|no line info",
						"text" : "",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "146|line 146",
						"text" : "sulla Croce per i peccati degli uomini.",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "no line info|no line info",
						"text" : "",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "147|line 147",
						"text" : "Egli ci ha redenti e ci ha concesso la vita,",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "no line info|no line info",
						"text" : "",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "148|line 148",
						"text" : "una dimora nei cieli. La speranza fu rinnovata",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "no line info|no line info",
						"text" : "",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "149|line 149",
						"text" : "con gloria e gioia per chi soffrì nel fuoco.",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "no line info|no line info",
						"text" : "",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "150|line 150",
						"text" : "il Figlio fu vittorioso in quella spedizione,",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "no line info|no line info",
						"text" : "",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "151|line 151",
						"text" : "potente e trionfante, quando con molti giunse,",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "no line info|no line info",
						"text" : "",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "152|line 152",
						"text" : "un esercito di anime, nel regno di Dio,",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "no line info|no line info",
						"text" : "",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "153|line 153",
						"text" : "sovrano onnipotente, per la gioia degli angeli",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "no line info|no line info",
						"text" : "",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "154|line 154",
						"text" : "e di tutti quei santi che già nei cieli",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "no line info|no line info",
						"text" : "",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "155|line 155",
						"text" : "dimoravano nella gloria, quando il loro Signore,",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "no line info|no line info",
						"text" : "",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "156|line 156",
						"text" : "Dio onnipotente, fece ritorno alla sua patria.",
						"tags" : "VB_text_DOTR|The Dream of the Rood",
						"loc" : "",
						"inFront": "",
						"lang": "ita"
					},
				
					{ 
						"line" : "no line info|no line info",
